<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
   
     
   
     <package name="default" namespace="/" extends="struts-default, json-default"> 
         <default-action-ref name="index"></default-action-ref>
         <action name="index" >
          <result>collaborateur/indexCollaborateur.jsp</result>  
         </action>
     </package>             
     <package name="collaborateur" namespace="/collaborateur" extends="struts-default, json-default">
			<action name="ConsulterFB" class="org.sqli.web.FeedbackAction" method="execute">
			<result name="success" type="json">indexCollaborateur.jsp</result>
			</action>
     </package>
	     
    
   

    <!-- Add addition packages and configuration here. -->
</struts>
